<!------------ Home Banner ------------>
<section id="banner" class="parallax" >
  <div class="gradient-overlay">
    <div class="container">
      
         <div id="wrapper">
      		<div>
      			<div class="sliderbutton"><img src="left.gif" width="32" height="38" alt="Previous" onclick="slideshow.move(-1)" /></div>
      			<div id="slider">
      				<ul>
      					<li><img src="http://static.wixstatic.com/media/b70cda_fac2e5e3819f4b5299d2decf3114be3d~mv2.png_srz_771_432_85_22_0.50_1.20_0.00_png_srz" width="500" height="300" alt="Sea turtle" /></li>
